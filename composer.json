{
  "name": "nicemodules/core",
  "description": "NiceModules core plugin",
  "keywords": [
    "wordpress",
    "framework",
    "plugin",
    "template"
  ],
  "minimum-stability": "stable",
  "license": "WTFPL",
  "authors": [
    {
      "name": "jedrek",
      "email": "nicemodules@gmail.com"
    }
  ],
  "require": {
    "composer/composer": "2.5.5",
    "nicemodules/wordpress-orm": "dev-master",
    "doctrine/annotations": "^2.0",
    "symfony/translation": "^5.4",
    "deeplcom/deepl-php": "^1.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:nicemodules/wordpress-orm.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "NiceModules\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  }
}
